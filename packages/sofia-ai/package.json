{
  "name": "sofia-ai",
  "version": "0.1.0",
  "description": "SDK JavaScript/TypeScript oficial para a API REST do Sofia AI",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "sofia-ai",
    "ai",
    "agents",
    "orchestration",
    "llm",
    "sdk"
  ],
  "author": "ROI Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JeanZorzetti/sofia-ia"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
